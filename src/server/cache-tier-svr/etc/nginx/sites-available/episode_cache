server {
    listen 80;
    server_name downloads.talkpythontome.com;
    charset utf-8;
    client_max_body_size 150M;

    location /static {
        alias /webapps/talk-python-to-me/src/episode_cache/episode_cache_app/static/;
    }
    location / { try_files $uri @yourapplication; }
    location @yourapplication {
        include uwsgi_params;
        uwsgi_pass unix:/tmp/episode-cache.sock;
    }
}
